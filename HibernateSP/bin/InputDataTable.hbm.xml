<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.shagg.java.hib.impl.InputDataTable" table="input_data_table">
   <id name="id" type="java.lang.String" column="ID" >
   <generator class="assigned"/>
  </id>
	
  <property name="x" type="java.lang.Float">
   <column name="x" />
  </property>
  <property name="y" type="java.lang.Float">
  <column name="y"/>
  </property>
 </class>
</hibernate-mapping>